{
  "name": "@jurijzahn8019/action-dependabot-labels",
  "version": "0.1.3",
  "description": "labels dependabot pull requests",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && for S in rollup types pkg; do npm run build:$S; done",
    "build:rollup": "rollup -c",
    "build:types": "tsc -p tsconfig.prod.json -d --outDir ./dist --emitDeclarationOnly",
    "build:tsc": "tsc --build tsconfig.prod.json",
    "build:watch": "tsc -w",
    "build:pkg": "cpy package*.json dist && npm ci --production --prefix dist",
    "clean": "rm -rf ./dist ./tmp",
    "lint": "eslint --fix src/**/*.ts",
    "precommit": "npm run lint && npm run build && git add dist/*",
    "prerel": "standard-version --skip.changelog -p $(git branch --show-current | sed 's/master/rc/g' | sed 's/[_\\/]/-/g')",
    "release": "standard-version",
    "push:tags": "git push --follow-tags origin master",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jurijzahn8019/action-dependabot-labels.git"
  },
  "author": "Jurij Zahn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jurijzahn8019/action-dependabot-labels/issues"
  },
  "homepage": "https://github.com/jurijzahn8019/action-dependabot-labels#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run precommit"
    }
  },
  "dependencies": {
    "@actions/core": "^1.2.4",
    "@actions/github": "^3.0.0",
    "@octokit/rest": "^18.0.0",
    "@octokit/types": "^5.0.1",
    "debug": "^4.1.1",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@rollup/plugin-typescript": "4.1.2",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@types/semver": "^7.2.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "cpy-cli": "^3.1.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.19.0",
    "eslint-plugin-prettier": "^3.1.4",
    "git-branch": "^2.0.1",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-junit": "^10.0.0",
    "prettier": "^2.0.5",
    "rollup": "^2.16.1",
    "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.9",
    "standard-version": "^8.0.0",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
